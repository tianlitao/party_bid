%header 报名
  .header-left
    %button.btn-style.btn-4(ng-click="back()") 返回
  .header-right( ng-switch="apply_status")
    %button.btn.btn-primary(ng-switch-when="2" ng-click="begain()" ng-disabled="disabled")开始
    %button.btn.btn-primary(ng-switch-when="1" ng-click="end()")结束　
#wrapper.wrapper.scrollable
  %form.wrapper-inner
    %div.wrapper
      %ul.list-style-2
        %li(ng-repeat="apply in applys" style="height:40px") {{apply.apply_phone}}           {{apply.apply_name}}
%footer
  %ul.footer-nav.footer-nav-2tabs
    %li.active
      %a(href="#/bidding")
        %i.icon-group 报名
    %li
      %a(href="#/bidding_now")
        %i.icon-comment 竞价
